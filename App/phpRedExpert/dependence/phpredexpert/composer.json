{
    "name": "eugef/phpredexpert",
    "description": "Lightweight and powerful web tool for Redis key-value store management and administration.",
    "authors": [
        {
            "name": "Eugene Fidelin",
            "email": "eugene.fidelin@gmail.com"
        }
    ],
    "license": "GPL-2.0",
    "type": "project",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "ext-redis": ">=2.2.3",
        "symfony/symfony": "~2.4",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",
        "components/angular.js": "~1.2",
        "components/angular-cache": "*",
        "components/angular-bootstrap": "*",
        "components/angular-ui-utils-unique" : "*",
        "components/bootstrap-css" : "*"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "component-dir": "src/Eugef/PhpRedExpert/AngularBundle/Resources/public/vendor"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "components/angular-cache",
                "type": "component",
                "version": "2.3.4",
                "dist": {
                    "url": "https://github.com/jmdobry/angular-cache/archive/2.3.4.zip",
                    "type": "zip"
                },
                "extra": {
                    "component": {
                        "files": [
                            "src/angular-cache.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/angular-bootstrap",
                "type": "component",
                "version": "0.11",
                "dist": {
                    "url": "https://github.com/angular-ui/bootstrap-bower/archive/0.11.0.zip",
                    "type": "zip"
                },
                "extra": {
                    "component": {
                        "files": [
                            "ui-bootstrap-tpls.js",
                            "ui-bootstrap-tpls.min.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/angular-ui-utils-unique",
                "type": "component",
                "version": "0.1.1",
                "dist": {
                    "url": "https://github.com/angular-ui/ui-utils/archive/unique-0.1.1.zip",
                    "type": "zip"
                },
                "extra": {
                    "component": {
                        "files": [
                            "unique.js",
                            "unique.min.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "components/bootstrap-css",
                "type": "component",
                "version": "3.0.3",
                "dist": {
                    "url": "https://raw.githubusercontent.com/twbs/bootstrap/v3.0.3/dist/css/bootstrap.css",
                    "type": "file"
                },
                "extra": {
                    "component": {
                        "files": [
                            "bootstrap.css"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        }
    ]
}
